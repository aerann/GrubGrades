<!-- Shows detail of a particular dish -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Show</title>
</head>
<body>
    <h1><%= dish.title %> </h1>
    <h1><%= dish.location %> </h1>
    <p><a href="/dishes/<%=dish._id%>/edit">Edit</a></p>
    <p>
        <form action="/dishes/<%=dish._id%>?_method=DELETE" method="POST">
            <button>Delete</button>
        </form>
    </p>
    <footer>
        <a href="/dishes">All Dishes</a>
    </footer>
    
</body>
</html>